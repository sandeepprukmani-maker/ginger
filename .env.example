# OpenAI API Key - Required for browser-use AI automation (uses GPT-4o)
OPENAI_API_KEY=your_openai_api_key_here

# Flask session secret - Required for secure sessions
SESSION_SECRET=your_secret_key_here

# Database URL - SQLite by default (automatically created)
DATABASE_URL=sqlite:///data/automation.db

# Optional: Flask environment (development or production)
FLASK_ENV=development
