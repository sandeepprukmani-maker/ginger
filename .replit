modules = ["python-3.11", "web", "nodejs-20"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["gitFull", "openssl", "playwright-driver", "postgresql"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "gunicorn --bind 0.0.0.0:5000 --reuse-port --reload main:app"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36903
externalPort = 6000

[[ports]]
localPort = 37223
externalPort = 5000

[[ports]]
localPort = 37899
externalPort = 3000

[[ports]]
localPort = 41419
externalPort = 5173

[[ports]]
localPort = 43723
externalPort = 3002

[[ports]]
localPort = 43817
externalPort = 3001

[[ports]]
localPort = 44407
externalPort = 4200

[[ports]]
localPort = 45181
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "--bind", "0.0.0.0:5000", "main:app"]
