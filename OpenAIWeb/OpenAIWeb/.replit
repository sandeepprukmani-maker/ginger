modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["chromium", "gitFull", "playwright-driver"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "CLI Info"

[[workflows.workflow]]
name = "CLI Info"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"AI Browser Automation CLI is ready!\" && echo \"\" && echo \"Usage examples:\" && echo \"  uv run python main.py 'go to example.com and get the title' --verbose\" && echo \"  uv run python main.py 'search for Python on Google' --generate-code --output automation.py\" && echo \"\" && echo \"Run 'uv run python main.py --help' for all options\""

[workflows.workflow.metadata]
outputType = "console"
